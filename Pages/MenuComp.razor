@page "/menucomp"
@layout MainLayout

@using DauaPharm.Data;
@using DauaPharm.Data.Entities;
@using Syncfusion.Blazor.Navigations
@inject AppState AppState

@inject IPharmService PharmService


<div id="ControlRegion">
    <div class="menu-control">
        <SfMenu Items="@menus"></SfMenu>
    </div>
</div>

@code{


    [Parameter]
    public int BuxFrm { get; set; }
    [Parameter]
    public int BuxKod { get; set; }

    // Create and empty list, named viideos, of empty Video objects.
    IEnumerable<Menu> menus;
    protected override async Task OnInitializedAsync()
    {
        //Populate the list of video objects from the Video table.
        BuxFrm = AppState.BuxFrm;
        BuxKod = AppState.BuxKod;

        menus = await PharmService.Pharm_GetMenu(BuxFrm, BuxKod);
    }

}
